{
  "version": "independent",
  "npmClient": "yarn",
  "packages": ["packages/**/*"],
  "changelogPreset": {
    "name": "conventionalcommits"
  },
  "ignoreChanges": ["**/__fixtures__/**", "**/__tests__/**", "**/*.md"],
  "command": {
    "version": {
      "allowBranch": ["master", "dev"],
      "message": "chore(release): publish %s"
    }
  }
}

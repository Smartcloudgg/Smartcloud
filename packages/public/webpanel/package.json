{
  "name": "@videndum/smartcloud-webpanel",
  "main": "node_modules/expo/AppEntry.js",
  "version": "0.0.1",
  "description": "Powers the user interface and administration panel",
  "author": "Jonathan Stevens <jonathan@videndum.studio>",
  "homepage": "https://smartcloud.gg/packages/UI",
  "license": "GPL-3.0-or-later",
  "scripts": {
    "start": "concurrently --kill-others \"start-storybook --ci -p 9009 -s ./src/assets\" \"wait-on http://localhost:9009 && expo start\" \"wait-on http://localhost:9009 && expo-electron start\"",
    "start:expo": "expo start",
    "start:web": "expo start:web",
    "start:android": "expo start --android",
    "start:ios": "expo start --ios",
    "start:desktop": "expo-electron start",
    "start:storybook": "start-storybook --ci -p 9009 -s ./src/assets",
    "build:web": "gatsby build",
    "build:storybook": "build-storybook -s ./src/assets -o .out",
    "server:web": "gatsby serve",
    "eject": "expo eject",
    "test": "jest --watchAll"
  },
  "jest": {
    "preset": "jest-expo"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@expo/vector-icons": "^10.0.0",
    "@react-native-community/masked-view": "0.1.10",
